<urn:test>
    a lv2:Plugin ,
        lv2:Plugin ;
    doap:name "Test";
    guiext:ui <urn:test#gui>;

    lv2:minorVersion 1;
    lv2:microVersion 0;

<urn:test#gui>
  a guiext:X11UI;
  guiext:binary <test_ui.so>;
        lv2:extensionData guiext::idle ;
        lv2:extensionData guiext:resize ;
        lv2:extensionData guiext:idleInterface;
        lv2:requiredFeature guiext:idleInterface .
